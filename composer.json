{
    "name": "icpabelona/flunky",
    "require": {
        "league/flysystem": "^1.0",
        "symfony/yaml": "^3.3",
        "tightenco/collect": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Flunky\\": "src/Flunky/"
        }
    },
    "autoload-dev": {
        "classmap": []
    },
    "scripts": {
        "post-install-cmd": [
            "bash scripts/init.sh",
            "bash contrib/setup.sh"
        ],
        "post-update-cmd": [
            "bash scripts/init.sh",
            "bash contrib/setup.sh"
        ]
    },
    "authors": [
        {
            "name": "Ivan Clint A. Pabelona",
            "email": "ipabelona@gmail.com"
        }
    ]
}
